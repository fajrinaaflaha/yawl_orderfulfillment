        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>false</initialValue>
      </localVariable>
      <localVariable>
        <index>5</index>
        <name>DeliveryInstructions</name>
        <type>DeliveryInstructionsType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>&lt;ShipmentNumber/&gt;
 &lt;DeliveryDate&gt;2010-06-02&lt;/DeliveryDate&gt;
 &lt;DeliveryInstructions/&gt;
 &lt;DeliveryLocation/&gt;</initialValue>
      </localVariable>
      <localVariable>
        <index>6</index>
        <name>BillOfLading</name>
        <type>BillOfLadingType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>7</index>
        <name>TrailerUsage</name>
        <type>TrailerUsageType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>&lt;OrderNumber/&gt;
 &lt;Packages&gt;
    &lt;Package&gt;
       &lt;PackageID/&gt;
       &lt;Volume&gt;25&lt;/Volume&gt;
    &lt;/Package&gt;
 &lt;/Packages&gt;</initialValue>
      </localVariable>
      <localVariable>
        <index>8</index>
        <name>CarrierTimeout</name>
        <type>YTimerType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>9</index>
        <name>ShipmentInformationDocument</name>
        <type>ShipmentInformationDocumentType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>10</index>
        <name>RouteGuide</name>
        <type>RouteGuideType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>11</index>
        <name>PickupInstructions</name>
        <type>PickupInstructionsType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>&lt;ShipmentNumber/&gt;
 &lt;PickupDate&gt;2010-06-02&lt;/PickupDate&gt;
 &lt;PickupInstructions/&gt;
 &lt;PickupSpot/&gt;</initialValue>
      </localVariable>
      <localVariable>
        <index>12</index>
        <name>CarrierManifest</name>
        <type>CarrierManifestType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <processControlElements>
        <inputCondition id="C_15">
          <name />
          <flowsInto>
            <nextElementRef id="Calculate_Carrier_Timeout" />
          </flowsInto>
        </inputCondition>
        <task id="Calculate_Carrier_Timeout">
          <name>Calculate Carrier Timeout</name>
          <flowsInto>
            <nextElementRef id="C_3" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="C_4" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="C_7" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;a&gt;{4}&lt;/a&gt;" />
              <mapsTo>a</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;b&gt;{count(/Carrier_Appointment/POrder/Order/OrderLines/*)}&lt;/b&gt;" />
              <mapsTo>b</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;POrder&gt;{/Carrier_Appointment/POrder/*}&lt;/POrder&gt;" />
              <mapsTo>POrder</mapsTo>
            </mapping>
          </startingMappings>
          <resourcing>
            <offer initiator="user" />
            <allocate initiator="user" />
            <start initiator="user" />
          </resourcing>
          <decomposesTo id="Calculate_Carrier_Timeout" />
        </task>
        <condition id="C_3">
          <flowsInto>
            <nextElementRef id="Prepare_Route_Guide" />
          </flowsInto>
        </condition>
        <condition id="C_4">
          <flowsInto>
            <nextElementRef id="Estimate_Trailer_Usage" />
          </flowsInto>
        </condition>
        <condition id="C_7">
          <flowsInto>
            <nextElementRef id="Carrier_Timeout" />
          </flowsInto>
        </condition>
        <task id="Carrier_Timeout">
          <name>Carrier Timeout</name>
          <flowsInto>
            <nextElementRef id="OutputCondition_2" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <timer>
            <netparam>CarrierTimeout</netparam>
          </timer>
          <resourcing>
            <offer initiator="user" />
            <allocate initiator="user" />
            <start initiator="user" />
          </resourcing>
          <decomposesTo id="Carrier_Timeout" />
        </task>
        <task id="Estimate_Trailer_Usage">
          <name>Estimate Trailer Usage</name>
          <flowsInto>
            <nextElementRef id="C_6" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;OrderLines&gt;{/Carrier_Appointment/POrder/Order/OrderLines/*}&lt;/OrderLines&gt;" />
              <mapsTo>OrderLines</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;OrderNumber&gt;{/Carrier_Appointment/POrder/Order/OrderNumber/text()}&lt;/OrderNumber&gt;" />
              <mapsTo>OrderNumber</mapsTo>
            </mapping>
          </startingMappings>
